<Application x:Class="GifCreator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GifCreator"
             xmlns:vm ="clr-namespace:GifCreator.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
        <vm:Locator  x:Key="Locator"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/WpfInfrastructure;component/Assets/ModernUI.Light.xaml" />
                <ResourceDictionary Source="/WpfInfrastructure;component/Assets/ModernUI.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
